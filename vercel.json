{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "CI=false npm install --legacy-peer-deps && CI=false DISABLE_ESLINT_PLUGIN=true npm run build",
        "distDir": "build",
        "outputDirectory": "build",
        "framework": "create-react-app"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
